{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "properties": {
        "name": {
            "type": "string"
        },
        "age": {
            "type": "number"
        },
        "city": {
            "type": "string"
        },
        "email": {
            "type": "string"
        },
        "phone": {
            "type": "string"
        },
        "anArray": {
            "items": {
                "type": "number"
            },
            "type": "array"
        },
        "address": {
            "properties": {
                "street": {
                    "type": "string"
                },
                "city": {
                    "type": "string"
                },
                "state": {
                    "type": "string"
                },
                "zip": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "required": [
                "street",
                "city",
                "state",
                "zip"
            ]
        },
        "orders": {
            "items": true,
            "type": "array"
        },
        "isActive": {
            "type": "boolean"
        },
        "lastLogin": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "type": "object",
    "required": [
        "name",
        "age",
        "city",
        "email",
        "phone",
        "isActive",
        "lastLogin"
    ]
}
